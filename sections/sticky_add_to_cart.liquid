<div class="product-header_form-block w-form">
          {% form 'product', product, class: 'product-header_default-state' %}
<div class="columns w-row">
              <div class="column w-col w-col-6">
                <template x-for="(option, optionKey) in product.options" :key="optionKey">
    <div li-element="product-options-loop" class="product-header_option" data-ProductVariantsLoop="true">
                  <div class="pointure-variant-dropdown"><select li-element="product-option-input" class="select-field w-select" x-on:click="setVariant" :name="option" :data-name="option" type="radio" :value="variantName" :id="variantName.toString().normalize('NFD').replace(/[\u0300-\u036f]/g, '').toLowerCase().trim().replace(/\s+/g, '-').replace(/[^\w-]+/g, '').replace(/--+/g, '-')" :checked="selected_or_first_available_variant?.options.includes(variantName)" data-selectorType="radio" :class="firstOptionHasAvailableChildren[variantName] ? '' : 'data-disabled'">
                      <option value="product-option-name">23</option>
                    </select></div>
                </div></template>
              </div>
              <div class="column-2 w-col w-col-6">
                <template x-if="selected_or_first_available_variant.available">
            <div li-js-if="selected_or_first_available_variant.available" class="product-header_buy-wrappper">
        
                  <a li-element="add-to-cart" href="#" class="add-to-cart-button w-button" @click="addToCart($event, true)" :disabled="!addToCartButton">Panier</a>
                        </div>
    </template>
                <template x-if="!selected_or_first_available_variant.available">
            <div li-js-if="!selected_or_first_available_variant.available" class="product-header_out-of-stock">
        
                  <a href="#" class="button is-secondary max-width-full pointer-events-off w-button">Not in Stock</a>
                        </div>
    </template>
              </div>
            </div>
          {% endform %}
          <div class="hide w-form-done">
            <div>Thank you! Your submission has been received!</div>
          </div>
          <div class="hide w-form-fail">
            <div>Oops! Something went wrong while submitting the form.</div>
          </div>
        </div> {% schema %} {
    "tag": "section",
    "name": "sticky-add-to-cart",
    "class": "section-sticky-add-to-cart",
    "presets": [
        {
            "name": "sticky-add-to-cart",
            "category": "Liquify",
            "blocks": []
        }
    ],
    "enabled_on": {
        "templates": [
            "product"
        ]
    }
} {% endschema %}