<style>
  .product_component {
    display: flex;
     width: 100%;
     flex-direction: column;
     grid-auto-columns: 1fr;
     grid-column-gap: 2rem;
     grid-row-gap: 2rem;
     grid-template-columns: 0.5fr 1fr;
     grid-template-rows: auto
  }
  .product_component.medium {
    grid-column-gap: 2rem;
     grid-row-gap: 2rem;
     grid-template-columns: 1fr
  }
  .product_heading-wrapper {
    display: flex;
     width: 100%;
     flex-direction: row;
     justify-content: space-between;
     align-items: stretch;
     grid-auto-columns: 1fr;
     grid-column-gap: 5rem;
     grid-row-gap: 1rem;
     grid-template-columns: 1fr max-content;
     grid-template-rows: auto
  }
  @media screen and (max-width: 767px) {
    .product_heading-wrapper {
      flex-direction: column;
       justify-content: flex-start;
       align-items: flex-start;
       grid-row-gap: 2rem;
       grid-template-columns: 1fr
    }
  }
  .product_heading-wrapper.medium {
    position: static;
     grid-column-gap: 3rem
  }
  @media screen and (max-width: 479px) {
    .product_heading-wrapper {
      display: block
    }
  }
  .product_heading {
    flex-grow: 1;
     flex-shrink: 1;
     flex-basis: 0%}
  .product_heading.medium {
    margin-right: 3rem
  }
  @media screen and (max-width: 767px) {
    .product_heading {
      margin-right: 0rem
    }
  }
  .button {
    padding-top: 0.65rem;
     padding-right: 1.5rem;
     padding-bottom: 0.65rem;
     padding-left: 1.5rem;
     border-top-style: solid;
     border-top-width: 1px;
     border-top-color: #6e612b;
     border-right-style: solid;
     border-right-width: 1px;
     border-right-color: #6e612b;
     border-bottom-style: solid;
     border-bottom-width: 1px;
     border-bottom-color: #6e612b;
     border-left-style: solid;
     border-left-width: 1px;
     border-left-color: #6e612b;
     border-top-left-radius: 10px;
     border-top-right-radius: 10px;
     border-bottom-left-radius: 10px;
     border-bottom-right-radius: 10px;
     background-color: hsla(0, 0.00%, 80.00%, 0.00);
     font-family: Karla;
     color: #6e612b;
     font-weight: 500;
     text-align: center;
     white-space: nowrap
  }
  @media screen and (max-width: 479px) {
    .button {
      display: inline-block
    }
  }
  .product_list-item {
    flex-grow: 0;
     flex-shrink: 0;
     flex-basis: auto
  }
  .Div Block 5 {
    padding-top: 60px;
     padding-right: 20px;
     padding-bottom: 60px;
     padding-left: 20px
  }
  .max-width-large {
    width: 100%;
     max-width: 48rem
  }
  .heading-style-h1 {
    font-family: Karla;
     color: #6e612b;
     font-size: 2.8rem;
     line-height: 1;
     font-weight: 600
  }
  @media screen and (max-width: 767px) {
    .heading-style-h1 {
      font-size: 2.5rem
    }
  }
  .hide {
    display: none;
     flex-grow: 1;
     flex-shrink: 1;
     flex-basis: 0%}
  .product_list {
    display: grid;
     justify-items: stretch;
     align-items: stretch;
     grid-auto-columns: 1fr;
     grid-column-gap: 1rem;
     grid-row-gap: 1rem;
     grid-template-columns: 1fr 1fr 1fr 1fr;
     grid-template-rows: auto
  }
  @media screen and (max-width: 767px) {
    .product_list {
      grid-column-gap: 1.25rem;
       grid-row-gap: 3rem;
       grid-template-columns: 1fr 1fr
    }
  }
  .product_list.medium {
    grid-column-gap: 0.9rem;
     grid-template-columns: 1fr 1fr 1fr
  }
  @media screen and (max-width: 479px) {
    .product_list {
      grid-column-gap: 10px;
       grid-template-columns: 1fr 1fr
    }
  }
  .product_button-row {
    display: flex;
     justify-content: flex-end;
     flex-wrap: wrap;
     align-items: center
  }
  .product_button-row.medium {
    flex-grow: 0;
     flex-shrink: 0;
     flex-basis: auto
  }
  @media screen and (max-width: 767px) {
    .product_button-row {
      width: 100%;
       justify-content: flex-start
    }
  }
</style>
  <div class="Div Block 5">
    <div>
      <div class="product_component">
        <div class="product_heading-wrapper">
          <div class="product_heading">
            <div class="max-width-large">
              <h2 class="heading-style-h1">
{{ section.settings.text_heading }}
</h2>
            </div>
          </div>
          <div class="product_button-row hide-mobile-landscape">
            <a class="button" href="{{ section.settings.url_button_link }}"> {{ section.settings.text_button_text }} </a>
          </div>
        </div>
        <div class="product_list-wrapper" data-dyn-type="wrapper">
          <div class="product_list" data-dyn-type="list">
            {% for product in section.settings.collection_collection.products limit: section.settings.collection_collection_limit %}
<div class="product_list-item" data-dyn-type="item" data-dyn-grid="12">
    
            
</div>
{% endfor %}
          </div>
          <div class="hide" data-dyn-type="empty">
            <div>
                No items found.
            </div>
          </div>
        </div>
      </div>
    </div>
  </div> {% schema %} {
    "tag": "section",
    "name": "collection-product-d",
    "class": "Collection-product",
    "settings": [
        {
            "id": "text_heading",
            "type": "text",
            "label": "Heading",
            "default": "\n                  Short heading here\n              "
        },
        {
            "id": "text_button_text",
            "type": "text",
            "label": "Button-Text",
            "default": "\n                VOIR\u00a0TOUT\n            "
        },
        {
            "id": "url_button_link",
            "type": "url",
            "label": "Button-Link"
        },
        {
            "id": "collection_collection",
            "type": "collection",
            "label": "Collection"
        },
        {
            "type": "range",
            "id": "collection_collection_limit",
            "min": 1,
            "max": 99,
            "step": 1,
            "label": "Limit: Collection",
            "default": 6
        }
    ],
    "presets": [
        {
            "name": "collection-product-d",
            "category": "Liquify",
            "blocks": []
        }
    ]
} {% endschema %}
